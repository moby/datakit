  "astring"
  "cmdliner"
  "fmt"
  "logs"
  "tyxml" {>= "4.0.0"}
  "tls" {>= "0.9.0"}
  "conduit-lwt-unix" {>= "1.0.0"}
  "io-page"
  "pbkdf"
  "webmachine" {>= "0.4.0"}
  "session-redis-lwt" {>= "0.4.0"}
  "session-webmachine" {>= "0.4.0"}
  "redis-lwt"
  "asetmap"
  "github-unix" {>= "3.0.0"}
  "prometheus-app"
  "lwt" {>= "3.0.0"}
  "ppx_sexp_conv" {build}
  "crunch" {build}
  "datakit" {test & >= "0.12.0"}
  "irmin-unix" {test & >= "1.2.0"}
  "alcotest" {test}
  "cohttp-lwt-unix" {>= "1.0.0"}
]
available: [ocaml-version >= "4.03.0"]
